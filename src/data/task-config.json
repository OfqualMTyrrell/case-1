              ]
            },
            {
              "id": "assign-caseworker",
              "name": "Assign a caseworker",
              "order": 2,
              "questions": [
                {
                  "id": "caseworker-selection",
                  "type": "select",
                  "label": "Assign Caseworker",
                  "required": true,
                  "options": [
                    { "value": "", "label": "Select a caseworker..." },
                    { "value": "jane-lee", "label": "Jane Lee" },
