              "questions": [
                {
                  "id": "information-complete",
                  "type": "radio",
                  "label": "Is all required information present and correct?",
                  "required": true,
                  "options": [
                    { "value": "yes", "label": "Yes" },
                    { "value": "no", "label": "No" },
                    { "value": "partially", "label": "Partially" }
                  ]
                },
                {
                  "id": "missing-info-details",
                  "type": "textarea",
                  "label": "Details of missing or incorrect information",
                  "required": false,
                  "rows": 4,
                  "placeholder": "Please provide details of any missing or incorrect information..."
